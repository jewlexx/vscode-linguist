{
  "name": "linguist",
  "displayName": "linguist",
  "version": "0.1.0",
  "description": "",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "webpack",
    "lint": "eslint src --ext ts",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "yarn run package",
    "watch": "webpack --watch"
  },
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "languages",
          "name": "Languages",
          "when": "gitOpenRepositoryCount >= 1",
          "contextualTitle": "Languages Explorer"
        }
      ]
    }
  },
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "dependencies": {
    "axios": "0.26.1",
    "execa": "6.1.0"
  },
  "devDependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "ts-loader": "9.2.8",
    "typescript": "4.5.5",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "vscode": "^1.66.0"
  }
}
